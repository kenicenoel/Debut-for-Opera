{
  "name": "Debut",
  "description": "A customizable startpage for your browser that can replace Opera's speed dial",
  "version": "1.15.9.7",
  "author": "Kenice Noel",
  "incognito": "spanning",
  "permissions": ["tabs", "storage"],


  "background":
  {
    "scripts": ["scripts/background.js"]
  },

"options_page":"options.html",

  "icons":
  {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "manifest_version": 2
}
